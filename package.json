{
  "name": "nodegit-lfs",
  "version": "0.0.1",
  "description": "A wrapper for NodeGit to facilitate LFS Support",
  "main": "build/src/index.js",
  "scripts": {
    "build": "yarn eslint && yarn compile",
    "compile": "\"node_modules/.bin/babel\" src -d build/src",
    "eslint": "\"node_modules/.bin/eslint\" src test",
    "eslint-quick-fix": "\"node_modules/.bin/eslint\" src test --fix",
    "pretest": "yarn eslint",
    "src-watch": "\"node_modules/.bin/babel\" src -d -w build/src",
    "test": "\"node_modules/.bin/nyc\" \"node_modules/.bin/mocha\" --compilers js:babel-core/register --expose-gc test/tests/*",
    "test-debug": "\"node_modules/.bin/mocha\" --debug-brk --compilers js:babel-core/register --expose-gc test/tests/*",
    "test-watch": "\"node_modules/.bin/mocha\" --compilers js:babel-core/register --expose-gc --watch test/tests/*"
  },
  "repository": "git@github.com:Axosoft/nodegit-lfs.git",
  "author": "Axosoft, LLC",
  "license": "ISC",
  "keywords": [
    "node",
    "git",
    "lfs",
    "nodegit"
  ],
  "directories": {
    "build": "./build",
    "test": "./test"
  },
  "bugs": {
    "url": "https://github.com/Axosoft/nodegit-lfs/issues"
  },
  "homepage": "https://github.com/Axosoft/nodegit-lfs#readme",
  "peerDependencies": {
    "nodegit": ">=0.18.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mocha": "^4.11.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "fs-extra": "^3.0.1",
    "promisify-node": "^0.4.0",
    "ramda": "^0.24.1",
    "minimatch": "^3.0.4"
  }
}
